{

    "manifest_version": 2,
    "name": "Youtube loop A-B",
    "version": "1.0",
  
    "description": "Trash addon",

    "icons": {
      "48": "logo-48.png"
    },

    "browser_action": {
      "default_icon": {
        "48": "logo-48.png"
      },
      "default_title": "Youtube Loop A-B"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/watch*"],
        "js": ["Main.js"]
      }
    ]
    
  
  }